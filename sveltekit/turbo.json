{
  "extends": [
    "//"
  ],
  "globalPassThroughEnv": [
    "PUBLIC_BURNER_WALLET_KEY_MODERATOR",
    "PUBLIC_BURNER_WALLET_KEY_UPLOADER",
    "PUBLIC_BURNER_WALLET_KEY_PINNER"
  ],
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true,
      "inputs": [
        "src/**"
      ],
      "outputLogs": "new-only"
    },
    "start": {
      "dependsOn": [
        "check"
      ],
      "cache": false,
      "persistent": true,
      "inputs": [
        "src/**"
      ],
      "outputLogs": "new-only"
    },
    "preview": {
      "dependsOn": [
        "build"
      ],
      "cache": false,
      "persistent": true,
      "outputLogs": "new-only"
    }
  }
}