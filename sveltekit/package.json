{
  "name": "@kredeum/massa-storage-sveltekit",
  "version": "0.0.2",
  "description": "Massa off-chain Storage on IPFS",
  "homepage": "https://github.com/kredeum/massa-storage",
  "license": "MIT",
  "author": "kredeum <contact@kredeum.com> (http://labs.kredeum.com/)",
  "type": "module",
  "scripts": {
    "all": "pnpm run setup && pnpm run format && pnpm run check && pnpm run build && pnpm run test && pnpm run dev",
    "clean": "rm -rf node_modules .svelte-kit build",
    "setup": "pnpm run clean && pnpm install",
    "sync": "NODE_ENV=prod svelte-kit sync",
    "format": "prettier --write --list-different src",
    "check": "pnpm run check:prettier && pnpm run check:eslint && pnpm run check:svelte",
    "check:prettier": "prettier --check src",
    "check:eslint": "eslint src",
    "check:svelte": "pnpm run sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "start": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "prettier --check src && eslint src",
    "test:unit": "vitest --run",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "playwright test"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@fastify/cors": "^10.0.2",
    "@helia/pinning-service-api-server": "^1.0.1",
    "@helia/remote-pinning": "^2.0.1",
    "@helia/strings": "^4.0.2",
    "@massalabs/as-types": "^2.1.0",
    "@playwright/test": "^1.50.1",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.17.2",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tsconfig/svelte": "^5.0.4",
    "@types/jszip": "^3.4.1",
    "autoprefixer": "^10.4.20",
    "blockstore-idb": "^2.0.1",
    "datastore-idb": "^3.0.1",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "fastify": "^5.2.1",
    "globals": "^15.15.0",
    "interface-blockstore": "^5.3.1",
    "it-all": "^3.0.6",
    "it-drain": "^3.0.7",
    "it-filter": "^3.1.1",
    "it-foreach": "^2.1.1",
    "it-merge": "^3.0.5",
    "kubo-rpc-client": "^5.1.0",
    "multiformats": "^13.3.2",
    "prettier": "^3.5.1",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.20.2",
    "svelte-check": "^4.1.4",
    "tailwindcss": "^3.4.17",
    "turbo": "^2.4.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.1.1",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vitest": "^3.0.6"
  },
  "dependencies": {
    "@eslint/js": "^9.20.0",
    "@helia/unixfs": "^4.0.2",
    "@ipld/dag-pb": "^4.1.3",
    "@kredeum/massa-storage-common": "workspace:^",
    "@massalabs/massa-web3": "^5.1.1",
    "@massalabs/wallet-provider": "^3.1.1",
    "buffer": "^6.0.3",
    "helia": "^5.2.1",
    "ipfs-unixfs": "^11.2.0",
    "jszip": "^3.10.1",
    "lucide-svelte": "0.344.0",
    "svelte-hot-french-toast": "^1.0.0",
    "svelte-portal": "^2.2.1"
  }
}
