    "storage",
    "offchain",
    "kredeum",
    "sveltekit",
    "svelte",
    "wasm"
  ],
  "homepage": "https://github.com/kredeum/massa-storage",
  "license": "MIT",
  "author": "kredeum <contact@kredeum.com> (http://labs.kredeum.com/)",
